{
  "name": "sahakum-khmer-cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/setup-schema.js && next dev",
    "build": "node scripts/setup-schema.js && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "sync-media": "npx tsx scripts/sync-media.ts",
    "db:generate": "node scripts/setup-schema.js && prisma generate",
    "db:studio": "prisma studio",
    "db:push": "node scripts/setup-schema.js && prisma db push",
    "db:reset": "node scripts/setup-schema.js && prisma db push --force-reset",
    "db:seed": "tsx scripts/seed-admin.ts",
    "db:migrate": "node scripts/setup-schema.js && prisma migrate deploy",
    "db:baseline": "node scripts/baseline-production.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "setup": "npm run docker:up && sleep 3 && npm run db:push && npm run db:seed",
    "reset": "npm run db:reset && npm run db:seed",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "postinstall": "node scripts/setup-schema.js && prisma generate"
  },
  "dependencies": {
